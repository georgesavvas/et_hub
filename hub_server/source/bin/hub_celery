#!/bin/bash

SCRIPT_DIR=$( cd -- "$( dirname -- "${BASH_SOURCE[0]}" )" &> /dev/null && pwd )

cd $SCRIPT_DIR/../python;
source ../../env/bin/activate;
source ../bin/env;
celery -A hub_server.celery worker --concurrency=1 -Q "hub" -n hub_worker$1 --pidfile /transfer/george_services/hub/celery/worker$1.pid;
