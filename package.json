{
  "name": "et_hub",
  "version": "0.0.1-alpha.1",
  "license": "Apache-2.0",
  "description": "ET Hub",
  "author": "George Savvas <george@electrictheatre.tv>",
  "main": "public/main.js",
  "homepage": ".",
  "dependencies": {
    "@emotion/react": "^11.8.2",
    "@emotion/styled": "^11.8.1",
    "@mui/icons-material": "^5.5.0",
    "@mui/lab": "^5.0.0-alpha.120",
    "@mui/material": "^5.5.0",
    "@mui/x-data-grid": "^5.17.24",
    "@nivo/core": "^0.80.0",
    "@nivo/pie": "^0.80.0",
    "@nivo/treemap": "^0.80.0",
    "@nivo/waffle": "^0.80.0",
    "@uiw/react-textarea-code-editor": "^2.1.1",
    "lodash.debounce": "^4.0.8",
    "node-os-utils": "^1.3.7",
    "notistack": "^2.0.4",
    "nuka-carousel": "^5.4.1",
    "react": "^18.2.0",
    "react-dnd": "^16.0.1",
    "react-dnd-html5-backend": "^16.0.1",
    "react-dom": "^18.2.0",
    "react-error-boundary": "^3.1.4",
    "react-grid-layout": "^1.3.4",
    "react-quill": "^2.0.0",
    "react-reflex": "^4.0.6",
    "react-resize-detector": "^8.0.4",
    "react-sortablejs": "^6.1.4",
    "react-spring": "^9.5.2",
    "react-virtualized-auto-sizer": "^1.0.7",
    "sockette": "^2.0.6",
    "uuid4": "^2.0.3",
    "v8-compile-cache": "^2.3.0",
    "valtio": "^1.5.2",
    "web-vitals": "^3.1.0"
  },
  "scripts": {
    "start": "cross-env BROWSER=none react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "pack": "electron-builder --dir",
    "dist": "electron-builder",
    "sanitize": "npx kill-port 3000",
    "electron": "cross-env NODE_ENV=dev electron .",
    "electron-dev": "npm run sanitize && concurrently \"npm start\" \"wait-on http://localhost:3000 && cross-env NODE_ENV=dev electron .\"",
    "package": "npm run build && electron-builder",
    "release": "electron-builder"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@electron/notarize": "^1.2.3",
    "@typescript-eslint/eslint-plugin": "^5.38.0",
    "@typescript-eslint/parser": "^5.38.0",
    "concurrently": "^7.2.2",
    "cross-env": "^7.0.3",
    "electron": "^22.0.0",
    "electron-builder": "^23.6.0",
    "eslint": "^8.24.0",
    "eslint-plugin-react": "^7.31.8",
    "react-scripts": "^5.0.1",
    "wait-on": "^6.0.1"
  },
  "build": {
    "appId": "georgesavvas.ethub",
    "productName": "et_hub",
    "artifactName": "${productName}-setup-${version}.${ext}",
    "generateUpdatesFilesForAllChannels": true,
    "afterSign": "scripts/notarize.js",
    "extraMetadata": {
      "main": "build/main.js"
    },
    "files": [
      "build/**/*"
    ],
    "directories": {
      "buildResources": "assets"
    },
    "linux": {
      "icon": "public/media/desktop_icon/linux/icon.png",
      "target": [
        "AppImage",
        "deb"
      ]
    }
  }
}
